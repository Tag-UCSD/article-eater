{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Article",
  "type": "object",
  "required": [
    "article_id",
    "title"
  ],
  "properties": {
    "article_id": {
      "type": "string"
    },
    "doi": {
      "type": [
        "string",
        "null"
      ]
    },
    "corpus_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "title": {
      "type": "string"
    },
    "authors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "venue": {
      "type": [
        "string",
        "null"
      ]
    },
    "year": {
      "type": [
        "integer",
        "null"
      ]
    },
    "abstract": {
      "type": [
        "string",
        "null"
      ]
    },
    "is_open_access": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "citation_count": {
      "type": [
        "integer",
        "null"
      ]
    },
    "url_pdf": {
      "type": [
        "string",
        "null"
      ]
    }
  }
}